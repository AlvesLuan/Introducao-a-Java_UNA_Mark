package aulamark0509;

public class IntroduçãoaAtribuição
{
    public static void main(String[] args) 
    {      
      int a ,b ,c;
      a = 10;
      b = 6;
      c = a - b;
      System.out.println(c);
        
      boolean resposta;
      resposta = (15 == 20);
      System.out.println(resposta);
                  
      int idade = 2022;
      System.out.println("Olá Mundo!");
      System.out.println("Tudo Bão? Sua idade é " + idade);
    }     
}

